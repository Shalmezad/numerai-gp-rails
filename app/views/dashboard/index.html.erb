<h3>Number to beat: 0.6931472</h3>
<table>
  <tr>
    <th>Deme ID</th>
    <th>Generation</th>
    <th>Size</th>
    <th>Running</th>
    <th>Best Gene</th>
    <th>Best LogLoss</th>
  </tr>
  <% @demes.each do |deme| %>
    <tr>
      <td><%= link_to deme.id, deme_path(deme) %></td>
      <td><%= deme.generation %></td>
      <td><%= deme.max_size %></td>
      <td><%= !deme.stop %></td>
      <td><%= deme.best_gene %></td>
      <td><%= '%.4f' % (deme.best_log_loss.to_f rescue 0) %></td>
    </tr>
  <% end %>
</table>
